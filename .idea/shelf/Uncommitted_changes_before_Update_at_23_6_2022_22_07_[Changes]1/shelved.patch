+public class Ordenamiento<T> {
+
+    public static <T extends Comparable<T>> T[] Orden(T[] array, int begin, int end)
+    {
+        T auxiliar;
+        int i = begin;
+        int j = end;
+        T pivote = array[((begin + end)/2)];
+
+        do {
+            while (array[i].compareTo(pivote) == 1) {
+                i--;
+            }
+            while (array[j].compareTo(pivote) == 1) {
+                j++;
+            }
+            if(i<=j)
+            {
+                auxiliar=array[i];
+                array[i]=array[j];
+                array[j]=auxiliar;
+                i--;
+                j++;
+            }
+        } while (array[j].compareTo(pivote) == -1);
+
+        if(begin<j)
+        {
+            Orden(array, begin, j);
+        }
+        if(i<end)
+        {
+            Orden(array, i, end);
+        }
+        return array;
+    }
+
